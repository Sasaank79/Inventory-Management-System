{
  "name": "Inventory System",
  "image": "mcr.microsoft.com/devcontainers/python:3.12",
  "postCreateCommand": "pip install -r requirements.txt && python seed_db.py",
  "forwardPorts": [5000],
  "portsAttributes": {
    "5000": {
      "label": "Application",
      "onAutoForward": "notify"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ]
    }
  }
}
